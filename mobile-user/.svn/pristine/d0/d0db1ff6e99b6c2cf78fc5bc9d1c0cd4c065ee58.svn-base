package com.fx.mobile.user.h5.controller;

import javax.servlet.http.HttpServletResponse;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.ModelAndView;

@Controller
@RequestMapping(value="/allapp")
public class ProtocalController {
	@RequestMapping("/protocal_protect")
     public ModelAndView toProtocalProtect( HttpServletResponse response){
    	 ModelAndView mav  = new ModelAndView("/allapp/protocal_protect");
 		 return mav;
    	 
     }
	@RequestMapping("/protocal_private")
    public ModelAndView toProtocalPrivate( HttpServletResponse response){
   	 ModelAndView mav  = new ModelAndView("/allapp/protocal_private");
		 return mav;
   	 
    }
}
