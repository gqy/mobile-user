<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
   
   <bean id="userOperateDao" class="com.fx.mobile.user.dao.impl.UserOperateDaoImp">
     <property name="sessionFactory" ref="sessionFactory" />
   </bean>
   
   <bean id="userInterfaceDao" class="com.fx.mobile.user.dao.impl.UserInterfaceDaoImpl">
     <property name="sessionFactory" ref="sessionFactory" />
   </bean>
   
   <bean id="noteNumRecordDao" class="com.fx.mobile.user.dao.impl.NoteNumRecordDaoImpl">
     <property name="sessionFactory" ref="sessionFactory" />
   </bean>
   
   <bean id="crashLogDao" class="com.fx.mobile.user.dao.impl.CrashLogDaoImpl">
     <property name="sessionFactory" ref="sessionFactory" />
   </bean>
</beans>